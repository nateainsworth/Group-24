@page
@model Group_24.Pages.Users.IndexModel
@{
}


<h1>Sign Up</h1>


<div>
    <form method="post">
        <div class="form-group">
            <label>First Name</label>
            <input asp-for="UserRecord.FirstName" class="form-control" />
        </div>
        <div class="form-group">
            <label>Last Name</label>
            <input asp-for="UserRecord.LastName" class="form-control" />
        </div>
        <div class="form-group">
            <label>Email</label>
            <input asp-for="UserRecord.EmailAddress" class="form-control" />
        </div>
        <div class="form-group">
            <label>Password</label>
            <input asp-for="UserRecord.Password" class="form-control" />
        </div>
        <button type="submit" class="btn btn-secondary">Sign Up</button>




    </form>
   

</div>